<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd"
	version="2.1">

	<application>
		<!-- configuracoes do locale (i18n) -->
		<locale-config>
			<default-locale>pt_BR</default-locale>
		</locale-config>

		<resource-bundle>
			<base-name>messages_labels</base-name>
			<var>msg</var>
		</resource-bundle>
	</application>

	<navigation-rule>
		<navigation-case>
			<from-outcome>incluirCompetencia</from-outcome>
			<to-view-id>/cadastros/competencia/incluirCompetencia.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>incluirVara</from-outcome>
			<to-view-id>/cadastros/vara/incluirVara.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>incluirClasse</from-outcome>
			<to-view-id>/cadastros/classe/incluirClasse.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>incluirComarca</from-outcome>
			<to-view-id>/cadastros/comarca/incluirComarca.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>processo</from-outcome>
			<to-view-id>/processo/distribuirProcesso.xhtml</to-view-id>
			<redirect />
		</navigation-case>		
	</navigation-rule>	
</faces-config>